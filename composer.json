{
    "type": "library",
    "name": "pamald/pamald",
    "description": "Generates a report about the diff between two package manager lock file content.",
    "keywords": [],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/pamald/pamald",
    "support": {
        "source": "https://github.com/pamald/pamald",
        "issues": "https://github.com/pamald/pamald/issues"
    },
    "license": "LGPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "preferred-install": {
          "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=8.2",
        "sweetchuck/utils": "2.x-dev"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "consolidation/robo": "^4.0",
        "maddhatter/markdown-table": "^1.2",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "3.x-dev",
        "phpstan/phpstan": "1.11.x-dev",
        "squizlabs/php_codesniffer": "^3.6",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpmd": "3.x-dev",
        "sweetchuck/robo-phpstan": "2.x-dev",
        "symfony/console": "^6.3",
        "symfony/error-handler": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/yaml": "^6.3"
    },
    "suggest": {
        "maddhatter/markdown-table": "^1.2 Required by \\Pamald\\Pamald\\Reporter\\MarkdownTableReporter",
        "symfony/console": "^6.0 Required by \\Pamald\\Pamald\\Reporter\\ConsoleTableReporter"
    },
    "autoload": {
        "psr-4": {
            "Pamald\\Pamald\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pamald\\Pamald\\Tests\\Unit\\": "tests/unit/",
            "Pamald\\Pamald\\Tests\\": "tests/_support/"
        }
    }
}
